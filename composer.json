{
  "name": "dyrynda/laravel-nullable-fields",
  "description": "This trait allows you to easily flag attributes that should be set as null when being persisted to the database using the Laravel PHP Framework.",
  "type": "utility",
  "require": {
    "php": "^8.1",
    "illuminate/database": "^10.0|^11.0",
    "illuminate/events": "^10.0|^11.0",
    "illuminate/contracts": "^10.0|^11.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Dyrynda",
      "email": "michael@dyrynda.com.au",
      "homepage": "https://dyrynda.com.au"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "extra": {
      "sort-packages": true
    }
  },
  "autoload": {
    "psr-4": {
      "Dyrynda\\Database\\Support\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0.7"
  }
}
